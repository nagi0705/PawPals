<h1>ユーザー一覧</h1>

<%= form_with url: search_users_path, method: :get, local: true do %>
  <%= label_tag :q, '検索ワード' %>
  <%= text_field_tag :q %>
  <%= submit_tag '検索', class: 'btn btn-primary' %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_path(user) %>
    </li>
  <% end %>
</ul>